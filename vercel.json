{
    "version": 2,
    "builds": [
        {
            "src": "run.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "run.py"
        },
        {
            "src": "/(.*)",
            "dest": "/app/static/$1"
        }
    ]
} 